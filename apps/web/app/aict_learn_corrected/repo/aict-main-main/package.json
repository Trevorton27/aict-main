{
  "name": "aict",
  "version": "0.1.0",
  "private": true,
  "description": "AI Coding Interactive Tutor - A pedagogical platform for learning web development",
  "scripts": {
    "dev": "pnpm --filter @aict/web dev",
    "build": "pnpm --filter @aict/web build",
    "start": "pnpm --filter @aict/web start",
    "lint": "pnpm --filter @aict/web lint",
    "type-check": "pnpm -r exec tsc --noEmit",
    "db:generate": "pnpm --filter @aict/web prisma generate",
    "db:push": "pnpm --filter @aict/web prisma db push",
    "db:seed": "pnpm --filter @aict/web prisma db seed",
    "db:studio": "pnpm --filter @aict/web prisma studio",
    "clean": "pnpm -r exec rm -rf node_modules .next dist",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "gen:challenges": "tsx scripts/generate-challenges.ts",
    "seed:challenges": "tsx scripts/seed-challenges.ts"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "prettier": "^3.1.0",
    "prisma": "5.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "dependencies": {
    "@prisma/client": "5.7.0"
  }
}