{
  "extends": "../apps/web/tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2020",
    "lib": ["ES2020"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@prisma/client": ["../apps/web/node_modules/@prisma/client"]
    }
  },
  "include": ["./**/*.ts"],
  "exclude": ["node_modules"]
}
